bash ./color_output.sh "++++++++++++++++++++++++++++++++" blue
bash ./locate_cursor.sh 5 20
bash ./color_output.sh "+      (1)Easy                 +" blue
bash ./locate_cursor.sh 6 20
bash ./color_output.sh "+      (2)Normal               +" blue
bash ./locate_cursor.sh 7 20
bash ./color_output.sh "+      (3)Hard                 +" blue
bash ./locate_cursor.sh 8 20
bash ./color_output.sh "+      (4)Exit                 +" blue
bash ./locate_cursor.sh 9 20
bash ./color_output.sh "++++++++++++++++++++++++++++++++" blue

